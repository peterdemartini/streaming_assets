{
    "name": "New ETL From Kafka to Kafka",
    "lifecycle": "persistent",
    "workers": 1,
    "assets": [
        "streaming"
    ],
    "operations": [
        {
            "_op": "kafka_stream_reader",
            "topic": "fixed-data-set",
            "connection": "default",
            "group": "new-kafka-etl-1",
            "size": 10000
        },
        {
            "_op": "map",
            "function": "setDate",
            "args": {
                "path": [
                    "processedAt"
                ],
                "timeFn": "now"
            }
        },
        {
            "_op": "kafka_stream_sender",
            "topic": "new-kafka-done",
            "timestamp_field": "created",
            "id_field": "uuid",
            "size": 10000
        }
    ],
    "tjm": {
        "cluster": "http://localhost:5678",
        "version": "0.0.1",
        "job_id": "f557d9ba-27c5-41cb-8b59-778e45306ed1"
    }
}
