{
    "name": "Old ETL From Kafka to Kafka",
    "lifecycle": "persistent",
    "workers": 1,
    "assets": [
        "streaming"
    ],
    "operations": [
        {
            "_op": "teraslic_kafka_reader",
            "topic": "fixed-data-set",
            "connection": "default",
            "group": "old-kafka-etl-1",
            "size": 10000
        },
        {
            "_op": "map",
            "function": "setDate",
            "args": {
                "path": [
                    "processedAt"
                ],
                "timeFn": "now"
            }
        },
        {
            "_op": "teraslice_kafka_sender",
            "topic": "old-kafka-done",
            "timestamp_field": "created",
            "id_field": "uuid",
            "size": 10000
        }
    ]
}
